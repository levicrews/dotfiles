# restart sxhkd
alt + shift + r
    killall -e sxhkd && sxhkd &

# rofi: default launcher
alt + shift + l
    launcher_text

# rofi: quicklinks
alt + shift + q
    menu_quicklinks

# rofi: screenshot
alt + shift + s
    menu_screenshot

# rofi: power
alt + shift + p
    menu_powermenu

# rofi: power
alt + shift + n
    menu_network

# rofi: sxhkd help
alt + shift + h
    sxhkd-help
