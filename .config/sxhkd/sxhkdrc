# restart sxhkd
super + shift + r
    killall -e sxhkd && sxhkd &

# rofi: default launcher
super + shift + l
    launcher_text

# rofi: quicklinks
super + shift + q
    menu_quicklinks

# rofi: screenshot
super + shift + s
    menu_screenshot

# rofi: power
super + shift + p
    menu_powermenu

# rofi: power
super + shift + n
    menu_network

# rofi: sxhkd help
super + shift + h
    sxhkd-help